package personalloan

import personalloan.LoanApplication
import personalloan.Applicant

rule "Underage"
	salience 10
	dialect "mvel"
	when
		application : LoanApplication( )
		Applicant( age < "21" )
	then
		application.setApproved( false );
		application.setExplanation( "Underage" );
		retract( application );
end
